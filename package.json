{
  "name": "@brewlab/calc",
  "version": "0.1.5",
  "description": "Library with methods for calculations used in brewing.",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 8
          },
          "modules": "commonjs",
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": "benblais"
  },
  "main": "index.js",
  "scripts": {
    "test": "jest --watch",
    "test:all": "jest src/*.test.js",
    "test:ci": "jest dist/*.test.js",
    "build": "babel src --out-dir dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bblais23/brewlab-calc.git"
  },
  "keywords": [
    "Brewing",
    "Claculations",
    "library"
  ],
  "author": "Ben Blais <me@benblais.dev>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/bblais23/brewlab-calc/issues"
  },
  "homepage": "https://github.com/bblais23/brewlab-calc#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "eslint": "^6.8.0",
    "eslint-config-benblais": "^1.2.1",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.2.3"
  },
  "dependencies": {
    "core-js": "^3.6.4"
  }
}
