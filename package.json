{
  "name": "@brewlab/calc",
  "version": "0.0.1",
  "description": "Library with methods for clalculations used in brewing.",
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 8
          },
          "modules": "commonjs",
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ]
  },
  "main": "index.js",
  "scripts": {
